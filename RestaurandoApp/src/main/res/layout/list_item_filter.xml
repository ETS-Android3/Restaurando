<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <android.support.v7.widget.AppCompatCheckBox
        android:id="@+id/list_item_filter_check_box_category"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/filter_checkbox_margin_left"
        android:layout_marginStart="@dimen/filter_checkbox_margin_left"
        android:checked="false"
        android:padding="@dimen/filter_checkbox_padding"
        style="@style/TextSubheading"
        android:textColor="@color/colorDarkText"
        app:buttonTint="@color/colorDialogButton"
        tools:text="None"/>
</LinearLayout>